	<rule  name="DoNotUseSystemFunction"
	 	language="java"
     since="5.0"
	  message="Do not use System.exit(), System.err(), System.out() functions"
	  class="net.sourceforge.pmd.lang.rule.XPathRule">
	  <description>
	  Do not use System.exit(), System.err(), System.out() functions.
	  </description>
	  <properties>
	    <property name="xpath">
	    <value>
	<![CDATA[
	/TypeDeclaration/ClassOrInterfaceDeclaration//PrimaryPrefix/Name
	[
	contains(@Image,'System.exit')
	or
	contains(@Image,'System.err')
	or
	contains(@Image,'System.out')
	]
	]]>
	    </value>
	    </property>
	  </properties>
	  <priority>3</priority>
	  <example>
	<![CDATA[
	package com.samsung.gmes2.sm.cmm.biz;
	
	public class getBizList()
	{
	   System.out(); // Do not use this function.
	   System.err(); // Do not use this function.
	   System.exit(); // Do not use this function.
	   
	   return;
	}
	]]>
	  </example>
	</rule>
